{
  "name": "insurance-game",
  "version": "0.2.8",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx,.vue",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "deploy": "gh-pages -d dist -b gh-pages",
    "prepare": "husky",
    "cui": "vite-node src/cui/cli.ts",
    "cui:play": "vite-node src/cui/cli.ts play",
    "cui:demo": "vite-node src/cui/cli.ts demo",
    "cui:tutorial": "vite-node src/cui/cli.ts tutorial",
    "cui:debug": "vite-node src/cui/cli.ts debug",
    "cui:help": "vite-node src/cui/cli.ts help-advanced"
  },
  "dependencies": {
    "pinia": "3.0.4",
    "vue": "^3.5.18",
    "vue-i18n": "11.1.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rushstack/eslint-patch": "^1.12.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/cli-progress": "^3.11.6",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.16.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@unocss/preset-attributify": "^66.3.3",
    "@unocss/preset-typography": "^66.3.3",
    "@unocss/preset-uno": "^66.3.3",
    "@unocss/reset": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "0.7.0",
    "boxen": "^8.0.1",
    "canvas": "^3.1.2",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-vue": "^9.33.0",
    "figlet": "^1.8.2",
    "gh-pages": "6.3.0",
    "husky": "^9.1.7",
    "inquirer": "^12.8.2",
    "jsdom": "^26.1.0",
    "lightningcss": "1.30.1",
    "ora": "^8.2.0",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "terser": "5.43.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}