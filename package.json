{
  "name": "insurance-game",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "task:complete": "cat docs/PRINCIPLES.md && echo '✅ タスク完了！'",
    "tc": "pnpm task:complete",
    "test:mcp": "node scripts/test-mcp.js",
    "mcp:start": "scripts\\start-mcp.bat"
  },
  "dependencies": {
    "phaser": "^3.90.0",
    "vue": "^3.5.14"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^22.10.7",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "@unocss/preset-attributify": "^66.3.3",
    "@unocss/preset-typography": "^66.3.3",
    "@unocss/preset-uno": "^66.3.3",
    "@unocss/reset": "^66.3.3",
    "@vitejs/plugin-vue": "^5.2.2",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.4.2",
    "typescript": "^5.6.3",
    "unocss": "^66.3.3",
    "vite": "^5.4.11",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.0"
  }
}