{
  "name": "insurance-game",
  "version": "0.2.4",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "task:complete": "cat docs/PRINCIPLES.md && echo '✅ タスク完了！'",
    "tc": "pnpm task:complete",
    "test:mcp": "node scripts/test-mcp.js",
    "mcp:start": "scripts\\start-mcp.bat",
    "cui": "vite-node src/cui/cli.ts",
    "cui:play": "vite-node src/cui/cli.ts play",
    "cui:demo": "vite-node src/cui/cli.ts demo",
    "cui:demo:fast": "vite-node src/cui/cli.ts demo --speed fast",
    "cui:demo:aggressive": "vite-node src/cui/cli.ts demo --strategy aggressive",
    "cui:tutorial": "vite-node src/cui/cli.ts tutorial",
    "cui:benchmark": "vite-node src/cui/cli.ts benchmark",
    "cui:benchmark:quick": "vite-node src/cui/cli.ts benchmark --quick",
    "cui:debug": "vite-node src/cui/cli.ts debug",
    "cui:help": "vite-node src/cui/cli.ts help-advanced",
    "cui:demo:auto": "vite-node src/cui/auto-demo.ts",
    "cui:demo:standalone": "vite-node src/cui/demo-standalone.ts",
    "advanced": "vite-node src/cli/AdvancedCLI.ts",
    "advanced:performance": "vite-node src/cli/AdvancedCLI.ts performance",
    "advanced:benchmark": "vite-node src/cli/AdvancedCLI.ts massive-benchmark",
    "advanced:analytics": "vite-node src/cli/AdvancedCLI.ts analytics",
    "advanced:ai": "vite-node src/cli/AdvancedCLI.ts ai",
    "advanced:research": "vite-node src/cli/AdvancedCLI.ts research",
    "advanced:status": "vite-node src/cli/AdvancedCLI.ts status",
    "benchmark:massive": "vite-node src/cli/AdvancedCLI.ts massive-benchmark quick",
    "benchmark:research": "vite-node src/cli/AdvancedCLI.ts massive-benchmark research",
    "benchmark:stress": "vite-node src/cli/AdvancedCLI.ts massive-benchmark stress",
    "analyze:performance": "vite-node src/cli/AdvancedCLI.ts performance analyze",
    "analyze:balance": "vite-node src/cli/AdvancedCLI.ts analytics balance",
    "optimize:performance": "vite-node src/cli/AdvancedCLI.ts performance optimize",
    "test:ai": "vite-node src/cli/AdvancedCLI.ts ai tournament",
    "experiment:run": "vite-node src/cli/AdvancedCLI.ts research experiment",
    "experiment:ab": "vite-node src/cli/AdvancedCLI.ts research ab-test",
    "test:cui": "vitest src/__tests__/cui --run",
    "test:controllers": "vitest src/__tests__/controllers --run",
    "test:performance": "vitest src/__tests__/performance --run",
    "test:analytics": "vitest src/__tests__/analytics --run",
    "test:integration": "vitest src/__tests__/integration --run",
    "test:stress": "vitest src/__tests__/stress --run --timeout=300000",
    "test:coverage:cui": "vitest --coverage src/__tests__/cui",
    "test:coverage:full": "vitest --coverage src/__tests__",
    "test:all:cui": "vitest src/__tests__ --run",
    "test:watch:cui": "vitest src/__tests__/cui",
    "test:benchmark": "vitest src/__tests__/benchmark --run --timeout=120000",
    "test:utils": "vitest src/__tests__/utils --run",
    "prepare": "husky"
  },
  "dependencies": {
    "phaser": "^3.90.0",
    "vue": "^3.5.18"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@rushstack/eslint-patch": "^1.12.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/cli-progress": "^3.11.6",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.16.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@unocss/preset-attributify": "^66.3.3",
    "@unocss/preset-typography": "^66.3.3",
    "@unocss/preset-uno": "^66.3.3",
    "@unocss/reset": "^66.3.3",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "boxen": "^8.0.1",
    "canvas": "^3.1.2",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-vue": "^9.33.0",
    "figlet": "^1.8.2",
    "husky": "^9.1.7",
    "inquirer": "^12.8.2",
    "jsdom": "^26.1.0",
    "ora": "^8.2.0",
    "phaser3spectorjs": "^0.0.8",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "vite": "^5.4.19",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
}
