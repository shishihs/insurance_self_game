{
  "name": "insurance-game",
  "version": "0.2.8",
  "private": true,
  "type": "module",
  "scripts": {
    "test:orchestrator": "node scripts/test-orchestrator.js",
    "test:orchestrator:stop-on-fail": "node scripts/test-orchestrator.js --stop-on-failure",
    "test:orchestrator:verbose": "node scripts/test-orchestrator.js --verbose",
    "test:orchestrator:no-e2e": "node scripts/test-orchestrator.js --skip-e2e",
    "test:full-suite": "node scripts/test-orchestrator.js --verbose --stop-on-failure",
    "test:quality-monitor": "node scripts/test-quality-monitor.js",
    "cui:integration": "node scripts/cui-auto-integration.js",
    "cui:integration:analysis": "node scripts/cui-auto-integration.js --analysis",
    "cui:integration:regression": "node scripts/cui-auto-integration.js --regression --analysis",
    "cui:integration:continuous": "node scripts/cui-auto-integration.js --continuous --analysis",
    "test:quality-dashboard": "node scripts/test-quality-monitor.js && start test-results/quality-dashboard.md",
    "test:complete-suite": "npm run test:orchestrator && npm run test:quality-monitor && npm run cui:integration:analysis",
    "dev": "vite",
    "dev:mobile": "vite --config vite.config.mobile.ts",
    "dev:mobile:host": "vite --config vite.config.mobile.ts --host",
    "build": "vue-tsc --noEmit && vite build",
    "build:mobile": "vue-tsc --noEmit && vite build --config vite.config.mobile.ts",
    "preview": "vite preview",
    "preview:mobile": "vite preview --config vite.config.mobile.ts --host",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:mobile": "node scripts/dev/test-mobile.js",
    "test:mobile:ui": "playwright test tests/e2e/mobile-responsive.spec.ts --ui",
    "setup:mobile": "node scripts/setup/mobile-setup.js",
    "task:complete": "cat docs/PRINCIPLES.md && echo '✅ タスク完了！'",
    "tc": "npm run task:complete",
    "test:mcp": "node scripts/test-mcp.js",
    "mcp:start": "scripts\\start-mcp.bat",
    "cui": "vite-node src/cui/cli.ts",
    "cui:play": "vite-node src/cui/cli.ts play",
    "cui:demo": "vite-node src/cui/cli.ts demo",
    "cui:demo:fast": "vite-node src/cui/cli.ts demo --speed fast",
    "cui:demo:aggressive": "vite-node src/cui/cli.ts demo --strategy aggressive",
    "cui:tutorial": "vite-node src/cui/cli.ts tutorial",
    "cui:benchmark": "vite-node src/cui/cli.ts benchmark",
    "cui:benchmark:quick": "vite-node src/cui/cli.ts benchmark --quick",
    "cui:debug": "vite-node src/cui/cli.ts debug",
    "cui:help": "vite-node src/cui/cli.ts help-advanced",
    "cui:demo:auto": "vite-node src/cui/auto-demo.ts",
    "cui:demo:standalone": "vite-node src/cui/demo-standalone.ts",
    "cui:playtest": "vite-node scripts/cui-playtest-runner.ts",
    "cui:playtest:simple": "vite-node scripts/cui-simple-playtest.ts",
    "cui:playtest:direct": "vite-node scripts/cui-direct-playtest.ts",
    "advanced": "vite-node src/cli/AdvancedCLI.ts",
    "advanced:performance": "vite-node src/cli/AdvancedCLI.ts performance",
    "advanced:benchmark": "vite-node src/cli/AdvancedCLI.ts massive-benchmark",
    "advanced:analytics": "vite-node src/cli/AdvancedCLI.ts analytics",
    "advanced:ai": "vite-node src/cli/AdvancedCLI.ts ai",
    "advanced:research": "vite-node src/cli/AdvancedCLI.ts research",
    "advanced:status": "vite-node src/cli/AdvancedCLI.ts status",
    "benchmark:massive": "vite-node src/cli/AdvancedCLI.ts massive-benchmark quick",
    "benchmark:research": "vite-node src/cli/AdvancedCLI.ts massive-benchmark research",
    "benchmark:stress": "vite-node src/cli/AdvancedCLI.ts massive-benchmark stress",
    "analyze:performance": "vite-node src/cli/AdvancedCLI.ts performance analyze",
    "analyze:balance": "vite-node src/cli/AdvancedCLI.ts analytics balance",
    "optimize:performance": "vite-node src/cli/AdvancedCLI.ts performance optimize",
    "test:ai": "vite-node src/cli/AdvancedCLI.ts ai tournament",
    "experiment:run": "vite-node src/cli/AdvancedCLI.ts research experiment",
    "experiment:ab": "vite-node src/cli/AdvancedCLI.ts research ab-test",
    "test:cui": "vitest src/__tests__/cui --run",
    "test:controllers": "vitest src/__tests__/controllers --run",
    "test:performance": "vitest src/__tests__/performance --run",
    "test:analytics": "vitest src/__tests__/analytics --run",
    "test:integration": "vitest src/__tests__/integration --run",
    "test:stress": "vitest src/__tests__/stress --run --timeout=300000",
    "test:coverage:cui": "vitest --coverage src/__tests__/cui",
    "test:coverage:full": "vitest --coverage src/__tests__",
    "test:all:cui": "vitest src/__tests__ --run",
    "test:watch:cui": "vitest src/__tests__/cui",
    "test:benchmark": "vitest src/__tests__/benchmark --run --timeout=120000",
    "test:utils": "vitest src/__tests__/utils --run",
    "test:security": "vitest run --config vitest.security.config.ts",
    "test:security:watch": "vitest --config vitest.security.config.ts",
    "test:security:coverage": "vitest run --config vitest.security.config.ts --coverage",
    "test:pipeline": "node scripts/test-pipeline-enhanced.js",
    "test:pipeline:parallel": "node scripts/test-pipeline-enhanced.js --parallel",
    "test:pipeline:ci": "node scripts/test-pipeline-enhanced.js --ci --fail-fast",
    "test:visual": "playwright test tests/visual",
    "test:visual:update": "playwright test tests/visual --update-snapshots",
    "test:performance:playwright": "playwright test tests/performance",
    "test:memory": "node scripts/memory-profiler.js",
    "test:memory:load": "node scripts/memory-profiler.js --with-load",
    "test:resource": "node scripts/resource-exhaustion.js",
    "test:all:parallel": "node scripts/test-pipeline-enhanced.js --parallel --detailed",
    "deploy:test": "node scripts/deployment-test.js",
    "deploy:test:quick": "node scripts/deployment-test.js --quick",
    "deploy:validate-pipeline": "node scripts/deployment-test.js --validate-only",
    "deploy": "gh-pages -d dist -b gh-pages",
    "prepare": "husky"
  },
  "dependencies": {
    "chart.js": "4.5.0",
    "phaser": "^3.90.0",
    "vue": "^3.5.18",
    "vue-chartjs": "5.3.2",
    "vue-i18n": "11.1.11"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rushstack/eslint-patch": "^1.12.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/vue": "^8.1.0",
    "@types/cli-progress": "^3.11.6",
    "@types/figlet": "^1.7.0",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.16.5",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@unocss/preset-attributify": "^66.3.3",
    "@unocss/preset-typography": "^66.3.3",
    "@unocss/preset-uno": "^66.3.3",
    "@unocss/reset": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "0.7.0",
    "boxen": "^8.0.1",
    "canvas": "^3.1.2",
    "chalk": "^5.4.1",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-vue": "^9.33.0",
    "figlet": "^1.8.2",
    "gh-pages": "6.3.0",
    "husky": "^9.1.7",
    "inquirer": "^12.8.2",
    "jsdom": "^26.1.0",
    "lightningcss": "1.30.1",
    "ora": "^8.2.0",
    "phaser3spectorjs": "^0.0.8",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "terser": "5.43.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unocss": "^66.3.3",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  }
